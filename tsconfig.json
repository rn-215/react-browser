{
  "compilerOptions": {
    "moduleResolution": "node",
    "noImplicitAny": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "es6",
    "target": "ES2016",
    "allowJs": true,
    "sourceMap": true,
    "types": [
      "jasmine"
    ],
    "baseUrl": ".",
    "paths": {
      "jslib/*": [ "jslib/src/*" ]
    }
  },
  "exclude": [
    "node_modules",
    "jslib/node_modules",
    "dist",
    "jslib/dist",
    "build",
    "build2",
    "build.safariextension",
    "coverage",
    "store",
    "src/edge"
  ]
}
